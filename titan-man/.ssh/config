Match !exec "nc -w 1 -z %h %p"
	ProxyJump kent@ssh.oslomet.no,kent@ssh-drift.oslomet.no
	IdentityFile ~/.ssh/id_ed25519
	ForwardAgent yes

Host *.oslomet.no
	User root

Host *.hioa.no
	User root

