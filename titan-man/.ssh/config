    PubkeyAuthentication no
#	AddressFamily inet
    User root

host *
	Port 22
	VerifyHostKeyDNS yes
    ForwardX11 no
    ForwardX11Trusted no
	ControlMaster auto
	ControlPersist 2h
	ControlPath ~/.ssh/controlmasters/%r@%h:%p
